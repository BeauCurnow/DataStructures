
@{
    ViewBag.Title = "Index";
}

<h2>Stack Index</h2>

<body>
    <div>
        <ul>
            <li>
                <a href="@Url.Action("AddOne", "Stack")" class="w3-bar-item w3-button">Add One Item to Stack</a>
            </li>
        </ul>
    </div>

    @foreach (var sData in ViewBag.MyStack)
    {
        <p>@sData</p>
    }
</body>
